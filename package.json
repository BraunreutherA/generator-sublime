{
    "name": "generator-sublime",
    "version": "0.1.0",
    "description": "Yeoman generator for scaffolfding the standard configuration root files like .gitignore, .jshintrc, .jscsrc etc...",
    "license": "MIT",
    "main": "app/index.js",
    "repository": "thaiat/generator-sublime",
    "author": {
        "name": "Avi Haiat",
        "email": "thaiat@ipelia.com",
        "url": "https://github.com/thaiat"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "scripts": {
        "prepublish": "bash ./deploy/prepublish.sh",
        "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha  && istanbul report cobertura",
        "test": "mocha"
    },
    "files": [
        "app",
        "templates"
    ],
    "preferGlobal": "true",
    "keywords": [
        "yeoman-generator",
        "editor",
        "app",
        "sublime",
        "lint",
        "jshint",
        "jsbeautify",
        "tern",
        "scaffold"
    ],
    "dependencies": {
        "chalk": "~0.4.0",
        "lodash": "^2.4.1",
        "strip-json-comments": "^1.0.1",
        "yeoman-generator": "~0.16.0",
        "yosay": "^0.1.0"
    },
    "devDependencies": {
        "mocha": "*"
    },
    "peerDependencies": {
        "yo": ">=1.0.0"
    }
}
